                                              //para crear al primer humano  "Rodrigo"	
	public static void main(String[] args) {
		MetodoSetter humano_chino=new MetodoSetter();
		
		MetodoSetter humano_chileno=new MetodoSetter();
		humano_chileno.establece_color_piel("negro");
		humano_chileno.establece_pais("chileno");
		humano_chileno.establece_salud("no");
		System.out.println(humano_chileno.dime_datos_generales());	
		
		System.out.println(humano_chileno.dime_pais());
		System.out.println(humano_chileno.dime_color());
		System.out.println(humano_chileno.dime_salud());
		
		
		
		
		
		
	}

}
